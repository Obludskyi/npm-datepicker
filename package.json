{
  "$schema": "./node_modules/ng-packagr/dist/package.schema.json",
  "name": "@coachcare/datepicker",
  "version": "0.3.0",
  "description": "Angular Material Datepicker Fork with Timepicker by CoachCare",
  "keywords": [
    "coachcare",
    "frontend",
    "datepicker"
  ],
  "author": "CoachCare.com",
  "homepage": "https://www.coachcare.com",
  "license": "MIT",
  "repository": "github:selvera/npm-datepicker",
  "bugs": {
    "url": "https://github.com/selvera/npm-datepicker/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rimraf dist && ng-packagr -p package.json",
    "commitmsg": "commitlint -e",
    "lint": "ng lint",
    "prerelease": "npm run build && node ./scripts/prerelease.js",
    "release": "standard-version --message 'build: release v%s'",
    "postrelease": "git push --follow-tags origin master",
    "upload": "npm publish --access=public dist/"
  },
  "devDependencies": {
    "@angular/animations": "~5.1.1",
    "@angular/cdk": "^5.0.4",
    "@angular/cli": "1.6.0",
    "@angular/common": "~5.1.1",
    "@angular/compiler": "~5.1.1",
    "@angular/compiler-cli": "~5.1.1",
    "@angular/core": "~5.1.1",
    "@angular/forms": "~5.1.1",
    "@angular/http": "~5.1.1",
    "@angular/language-service": "~5.1.1",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "~5.1.1",
    "@angular/platform-browser-dynamic": "~5.1.1",
    "@angular/router": "~5.1.1",
    "@commitlint/cli": "^5.0.0",
    "@commitlint/config-angular": "^5.0.0",
    "@types/moment-timezone": "^0.5.3",
    "core-js": "^2.4.1",
    "cpx": "^1.5.0",
    "fs-extra": "^5.0.0",
    "moment": ">= 2.18.1",
    "moment-timezone": ">= 0.5.14",
    "ng-packagr": "github:selvera/ng-packagr#customized",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.2",
    "standard-version": "^4.3.0",
    "tslint": "~5.9.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.14"
  },
  "peerDependencies": {
    "@angular/cdk": "*",
    "@angular/material": "*",
    "moment": "*",
    "moment-timezone": "*"
  },
  "ngPackage": {
    "dest": "./dist",
    "lib": {
      "entryFile": "./src/datepicker/index.ts",
      "umdModuleIds": {
        "@angular/cdk": "ng.cdk",
        "@angular/material": "ng.material",
        "lodash": "_",
        "moment": "moment",
        "moment-timezone": "moment-timezone"
      }
    }
  },
  "main": "dist/bundles/coachcare-datepicker.umd.js",
  "module": "dist/esm5/coachcare-datepicker.js",
  "es2015": "dist/esm2015/coachcare-datepicker.js",
  "typings": "dist/coachcare-datepicker.d.ts",
  "metadata": "dist/coachcare-datepicker.metadata.json",
  "files": [
    "dist"
  ]
}
